// 画像と名前+++++++++++++++++++++++++++++++++++++++++
function toNameFace1(n){
	var str = Names[n];
	var temp = "";

	temp = "0"+String(n+1);
	temp = (temp.length==4) ? temp.substr(1,3) : temp.substr(0,3);
	str = "<img src=\"./"+temp+".jpg?20180608\"><br>"+str;

	return str;
}

// 画像と名前 その２+++++++++++++++++++++++++++++++++++++++++
function toNameFace2(n,w){
	var str = Names[n];
	var temp = "";

	temp = "0"+String(n+1);
	temp = (temp.length==4) ? temp.substr(1,3) : temp.substr(0,3);
	str = "<img style=\"width:"+w+"px;\" src=\"./"+temp+".jpg?20180608\"><br>"+str;

	return str;
}

