// 数値をグループ名に変換+++++++++++++++++++++++++++++++++++++++++
function toNameFace(n){
	var str = Names[n];
	var temp = "";

	//顔文字を追加する場合は以下のコメントアウトを外す
	//namMemberのインデックスと矛盾しないように注意

	str += "<br>";
	switch(n) {
		//case -1 はサンプルなので削除すること
		case 00: str+="モーニング娘。'17[9期]9代目リーダー<br>ハロプロサブリーダー<br>(7代目サブリーダー,ハロプロエッグ[4期])";break;
		case 01: str+="モーニング娘。'17[9期]8代目サブリーダー";break;
		case 02: str+="モーニング娘。'17[10期]7,8代目サブリーダー";break;
		case 03: str+="モーニング娘。'17[10期]";break;
		case 04: str+="モーニング娘。'17[10期]";break;
		case 05: str+="モーニング娘。'17[11期]<br>(ハロプロ研修生[13期])";break;
		case 06: str+="モーニング娘。'17[12期]";break;
		case 07: str+="モーニング娘。'17[12期]";break;
		case 08: str+="モーニング娘。'17[12期]<br>(ハロプロ研修生[17期])";break;
