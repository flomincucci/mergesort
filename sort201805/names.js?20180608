		case 114: str+="Berryz工房ＯＧ<br>(ZYX,ハロプロキッズ)";break;
		case 115: str+="Berryz工房ＯＧ<br>(ハロプロキッズ)";break;
		case 116: str+="Berryz工房ＯＧ<br>(ハロプロキッズ)";break;
		case 117: str+="Berryz工房ＯＧ<br>(ハロプロキッズ)";break;
		case 118: str+="Berryz工房ＯＧ<br>(ハロプロキッズ)";break;
		case 119: str+="℃-uteＯＧ<br>(ZYX,ハロプロキッズ)";break;
		case 120: str+="℃-uteＯＧ<br>(ハロプロキッズ)";break;
		case 121: str+="℃-uteＯＧ<br>(Buono!,あぁ!,ハロプロキッズ)";break;
		case 122: str+="℃-uteＯＧ<br>(ハロプロキッズ)";break;
//		case 122: str+="ソロ";break;
		case 123: str+="カントリー・ガールズ スーパーバイザー<br>(カントリー娘。,音楽ガッタス)";break;
		case 124: str+="美勇伝ＯＧ";break;
		case 125: str+="アンジュルムＯＧ<br>(スマイレージ,ハロプロエッグ[1期])";break;
		case 126: str+="ソロ<br>(音楽ガッタス,ハロプロエッグ[2期])";break;
		case 127: str+="アップアップガールズ(仮)ＯＧ,<br>(チーム・負けん気,音楽ガッタス,ハロプロエッグ[1期])";break;
		case 128: str+="アップアップガールズ(２)[1期]";break;
		case 129: str+="アップアップガールズ(２)[1期]";break;
		case 130: str+="アップアップガールズ(２)[1期]";break;
		case 131: str+="アップアップガールズ(２)[1期]";break;
		case 132: str+="アップアップガールズ(２)[1期]";break;
		case 133: str+="アップアップガールズ(２)[2期]";break;
		case 134: str+="アップアップガールズ(２)[2期]";break;
		case 135: str+="アップアップガールズ(プロレス)";break;
		case 136: str+="アップアップガールズ(プロレス)";break;
		case 137: str+="アップアップガールズ(プロレス)";break;
		case 138: str+="アップアップガールズ(プロレス)";break;
		default: str+=""+n;
	}
	temp = "0"+String(n+1);
	temp = (temp.length==4) ? temp.substr(1,3) : temp.substr(0,3);
	str = "<img src=\"./"+temp+".jpg?20180608\" width=\"280\"><br>"+str;

	return str;
}

